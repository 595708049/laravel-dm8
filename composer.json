{
    "name": "july/laravel11-dm8",
    "description": "达梦数据库 DM8 的 Laravel 11 适配器",
    "license": "MIT",
    "authors": [
        {
            "name": "July",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-pdo": "*",
        "illuminate/database": "^10|^11",
        "illuminate/pagination": "^10|^11",
        "illuminate/support": "^10|^11",
        "illuminate/validation": "^10|^11"
    },
    "require-dev": {
        "doctrine/dbal": "^3.3|^4.0",
        "mockery/mockery": "^1.4.4|^1.6",
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^9.5.8|^10.0"
    },
    "autoload": {
        "files": [
          "src/helper.php"
        ],
        "psr-4": {
            "LaravelDm8\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelDm8\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
          "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "LaravelDm8\\Dm8\\Dm8ServiceProvider",
                "LaravelDm8\\Dm8\\Dm8ValidationServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
